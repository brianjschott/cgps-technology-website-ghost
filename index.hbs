{{!< default}}
{{!-- The tag above means: insert everything in this file
into the {body} of the default.hbs template --}}
{{!-- test --}}
{{!-- test2 --}}
{{#is "home"}}
    {{#if @site.description}}
    <header class="page-head">
        <h2 class="page-head-title">{{@site.title}}</h2>
        <p class="home-page-description">{{@site.description}}</p>
    </header>
    {{/if}}
{{/is}}
<div class="main-page-content-container">
    <div class="post-feed main-page-feed">
        {{#foreach posts}}
            {{#has tag="news"}}
            {{!-- The tag below includes the markup for each post - partials/post-card.hbs --}}
                {{> "post-card"}}
            {{/has}}
        {{/foreach}}
    </div>
    <div class="main-page-calendar">
        <!--<div id="gcf-simple"></div>-->
            
    </div>
</div>